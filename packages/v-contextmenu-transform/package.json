    "prepublishOnly": "npm run lint && npm run build:package",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "ghpages": "npm run build:example && gh-pages -b 2.x-gh-pages -d sites"
